{
  "name": "voting-backend",
  "version": "1.0.0",
  "description": "Backend of the voting app",
  "main": "index.js",
  "repository": "https://github.com/Organisasjonskollegiet/voting-backend",
  "author": "kharann",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --no-notify src/server.ts",
    "build": "tsc",
