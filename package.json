    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^6.0.5",
    "graphql-shield": "^7.5.0",
    "graphql-ws": "^4.3.2",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.1",
    "nexus": "^1.0.0",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
