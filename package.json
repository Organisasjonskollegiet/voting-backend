    "populatedb": "ts-node-dev --transpile-only --no-notify src/utils/populatedb"
  },
  "dependencies": {
    "@prisma/client": "^2.19.0",
    "apollo-server-express": "2.22.1",
    "bcrypt": "^5.0.1",
